
local D=Data
local C=D.Color
local TT=D.TriggerType

local W=D.__iw

local SP_X, SP_Y=16,16
local SP_C=C.System

local ED_X, ED_Y=1,1
local ED_C=C.Aqua
local ED_N="next_world_name"

D.S_SP(W, SP_X,SP_Y, SP_C)

-- Start trigger
D.M_TR(
	W, SP_X,SP_Y,
	TT.Message,
	"msg_start",
	{
		"Starter message.",
		starter=true
	}
)

-- End trigger
D.M_TR(
	W, ED_X,ED_Y,
	TT.ChangeWorld,
	"cw_end",
	{ED_N},
	ED_C
)
